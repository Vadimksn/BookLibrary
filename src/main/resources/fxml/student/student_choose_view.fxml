<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="apStudentChose" maxHeight="400.0" maxWidth="625.0" minHeight="400.0" minWidth="625.0"
            prefHeight="400.0" prefWidth="625.0"
            style="-fx-background-color: #d7d7d7;" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.student.StudentChooseController">
    <children>
        <TableView fx:id="tvChooseStudent" layoutY="63.0" prefHeight="345.0" prefWidth="480.0"
                   AnchorPane.bottomAnchor="55.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                   AnchorPane.topAnchor="0.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="tcId" editable="false" maxWidth="100.0" minWidth="75.0" prefWidth="75.0" text="ID"/>
                <TableColumn fx:id="tcSurname" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0"
                             prefWidth="150.0"
                             text="Прізвище"/>
                <TableColumn fx:id="tcName" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0"
                             prefWidth="150.0"
                             text="Ім'я"/>
                <TableColumn fx:id="tcMiddleName" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0"
                             prefWidth="150.0"
                             text="По батькові"/>
                <TableColumn fx:id="tcPassportData" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0"
                             prefWidth="100.0"
                             text="Паспортні дані"/>
            </columns>
        </TableView>
        <Button fx:id="btnОк" layoutX="301.0" layoutY="461.0" maxHeight="25.0" maxWidth="70.0" minHeight="25.0"
                minWidth="70.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Ок"
                AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="100.0"/>
        <Button fx:id="btnCancel" layoutX="369.0" layoutY="461.0" maxHeight="25.0" maxWidth="70.0" minHeight="25.0"
                minWidth="70.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Відміна"
                AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0"/>
        <TextField fx:id="tfSearch" layoutX="14.0" layoutY="415.0" maxHeight="25.0" maxWidth="1.7976931348623157E308"
                   minHeight="25.0" minWidth="140.0" prefHeight="25.0" prefWidth="140.0" promptText="Пошук"
                   AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0"/>
    </children>
</AnchorPane>
